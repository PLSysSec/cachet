fn negate_i32(x: Int32) -> Int32 {
    -x
}

fn negate_i64(x: Int64) -> Int64 {
    -x
}

fn add_i32(a: Int32, b: Int32) -> Int32 {
    a + b
}

fn add_i64(a: Int64, b: Int64) -> Int64 {
    a + b
}

fn add_u16(a: UInt16, b: UInt16) -> UInt16 {
    a + b
}

fn add_u64(a: UInt64, b: UInt64) -> UInt64 {
    a + b
}


fn infinity() -> Double {
    Double::INFINITY
}

fn neg_infinity() -> Double {
    Double::NEG_INFINITY
}

fn is_nan(d: Double) -> Bool {
    Double::is_nan(d)
}
