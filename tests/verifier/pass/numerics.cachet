fn test(d: Double) {
    assert 4_i32 > 3_i32;
    assert 4_i32 >= 3_i32;
    assert 4_i32 >= 4_i32;
    assert 3_i32 <= 4_i32;
    assert 4_i32 == 4_i32;
    assert 3_i32 != 4_i32;

    assert 1_i64 < 8_i64;
    assert 4_i64 > 3_i64;
    assert 4_i64 >= 3_i64;
    assert 4_i64 >= 4_i64;
    assert 3_i64 <= 4_i64;
    assert 4_i64 == 4_i64;
    assert 3_i64 != 4_i64;


    assert 1_u16 < 8_u16;
    assert 4_u16 > 3_u16;
    assert 4_u16 >= 3_u16;
    assert 4_u16 >= 4_u16;
    assert 3_u16 <= 4_u16;
    assert 4_u16 == 4_u16;
    assert 3_u16 != 4_u16;

    if !Double::is_nan(d) {
        assert Double::INFINITY >= d;
        assert Double::NEG_INFINITY <= d;
    }
}