import "../cacheir.cachet"
import "../utils.cachet"

ir CacheStub emits CacheIR {
    op Neg(
        inputValueId: ValueId,
    ) {
        initRegAllocator();
        initValueInput(inputValueId);
        initValueOutput();

        emit CacheIR::GuardToInt32(inputValueId);
        let inputInt32Id = ValueId::toInt32Id(inputValueId);
        emit CacheIR::Int32NegationResult(inputInt32Id);
    }
}
