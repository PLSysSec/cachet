import "../cacheir.cachet"

ir CacheStub emits CacheIR {
    op Neg(
        inputValueId: ValueId,
        inputInt32Id: Int32Id,
    ) {
        CacheIR::initValueInput(inputValueId);
        CacheIR::defineValueReg(inputValueId);
        CacheIR::initInt32Input(inputInt32Id);
        CacheIR::defineInt32Reg(inputInt32Id);

        emit CacheIR::GuardToInt32(inputValueId, inputInt32Id);
        emit CacheIR::Int32NegationResult(inputInt32Id);
    }
}
