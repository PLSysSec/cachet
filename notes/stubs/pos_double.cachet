import "../cacheir.cachet"
import "../utils.cachet"

ir CacheStub emits CacheIR {
    op Pos(
        inputId: ValueId,
    ) {
        initRegState();
        initValueInput(inputId);
        initValueOutput();

        emit CacheIR::GuardIsNumber(inputId);
        let valId = ValueId::toNumberId(inputId);
        emit CacheIR::LoadDoubleResult(valId);
    }
}
