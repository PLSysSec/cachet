import "../cacheir.cachet"

ir CacheStub emits CacheIR {
    op Pos(
        inputId: ValueId,
        valId: NumberId,
    ) {
        CacheIR::initValueInput(inputId);
        CacheIR::initNumberInput(valId);

        emit CacheIR::GuardIsNumber(inputId, valId);
        emit CacheIR::LoadDoubleResult(valId);
    }
}
