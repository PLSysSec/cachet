impl UInt16 {
    var max: UInt16 = 65535_u16;
}

impl Int16 {
    fn abs(n: Int16) -> Int16 {
        if n < 0_i16 {
            return -n;
        } else {
            return n;
        }
    }
}

impl Int32 {
    fn abs(n: Int32) -> Int32 {
        if n < 0_i32 {
            return -n;
        } else {
            return n;
        }
    }

    fn max(a: Int32, b: Int32) -> Int32 {
        if a < b {
            return b;
        } else {
            return a;
        }
    }
}

impl UInt32 {
    #[prelude]
    fn print(n: UInt32) {}
}

impl Double {
    #[prelude]
    var INFINITY: Double;

    #[prelude]
    var NEG_INFINITY: Double;

    #[prelude]
    fn is_nan(i: Double) -> Bool;

    #[prelude]
    fn bits(i: Double) -> UInt64;

    #[prelude]
    fn ceil(i: Double) -> Double;
}