            async call TA(cid);
        }
        yield;
        assert {:layer 3} counter == 0;
        if (*) {
            call cid := AllocTid();
            async call AbsTB(cid);
        }
        yield;
        assert {:layer 3} counter == 0;
        call LockA(tid);
        call AbsAssertA(tid);
        call LockB(tid);
        call AbsAssertB(tid);
        call UnlockB(tid);
        call UnlockA(tid);
    }
}
