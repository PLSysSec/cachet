procedure {:yields} {:layer 1}
{:yield_preserves "Yield"}
Thread({:linear "tid"} tid: X)
requires {:layer 1} tid != nil;
{
    var start, size: int;
    var bytesRead, i, tmp: int;

    havoc start, size;
    assume (0 <= start && 0 <= size);

    bytesRead := size;
