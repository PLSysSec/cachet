procedure {:yields} {:layer 0} {:refines "AtomicWriteNewsize"} WriteNewsize({:linear "tid"} tid: X, val: int);

procedure {:atomic} {:layer 1} AtomicWriteCurrsize({:linear "tid"} tid: X, val: int)
  modifies currsize, ghostLock;
{
