  modifies Heap;
  ensures (forall <alpha> o: ref, f: Field alpha :: Heap[o, f] == old (Heap)[o, f] || !old (Heap)[o, alloc] || (o == this && StrictEqual(f, F0)) || (o == this && LiberalEqual(f, f)) || (o == this && IntEqual(F0, f)));
{
