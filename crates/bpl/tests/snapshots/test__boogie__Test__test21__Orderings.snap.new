const a, b: int;

const c: int extends a, b;

procedure P() {
  var x: int;

  assert c <: a;
  assume c <: x && x <: a;
  assert x == c || a == x;
  assert x == b;
}

procedure Q() {
  assume b <: a;
  assert b == a;
}

