  assert n >= 0;

  assume
    {:add_to_pool "A", i, i+1}
    {:add_to_pool "B", ADD(n)}
    0 <= i && i <= n;
  x := x + (i * (i+1)) div 2;
  PAs := (lambda {:pool "B"} pa: PA :: if is#ADD(pa) && i < i#ADD(pa) && i#ADD(pa) <= n then 1 else 0);
  choice := ADD(i+1);
}

////////////////////////////////////////////////////////////////////////////////

procedure {:left}{:layer 1}
ADD (i: int)
modifies x;
{
  x := x + i;
}
