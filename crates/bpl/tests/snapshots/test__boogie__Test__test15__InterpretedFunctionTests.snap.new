  assert x + y == 4;
}

procedure subtraction(x: int, y: int) {
  assume x == 1;
  assume y == 2;
  assert x - y == 4;
}

procedure multiplication(x: int, y: int) {
  assume x == 1;
  assume y == 2;
  assert x * y == 4;
}

