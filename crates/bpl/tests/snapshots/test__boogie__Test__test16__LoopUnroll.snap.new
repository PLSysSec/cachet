[Printed]
procedure P() {
  var x: int;

  A:
  x := 0;
  goto B, Goner, C;
  B:
  x := 1;
  goto D;
  C:
  x := 2;
  goto D;
  Goner:
  x := 5;
  assume false;
  x := 6;
  goto B;
  D:
  x := 3;
  goto LoopHead;
  LoopHead:
  assert x < 100;
  goto LoopBody, LoopDone;
  LoopBody:
  x := x + 1;
  goto LoopHead, LoopBodyMore;
  LoopBodyMore:
  x := x + 2;
  goto LoopHead;
  LoopDone:
  x := 88;
  return;
}

type MyValue;

const SpecialValue: MyValue;

procedure WrongRange(a: [int]MyValue, N: int)
  requires 0 <= N;
{
  var i: int, v: MyValue;

  i := 1;
  while (i <= N) {
    assert 0 <= i;
    assert i < N;
    v := a[i];
    i := i + 1;
  }
}

procedure ManyIterations(a: [int]MyValue, N: int)
  requires 0 <= N;
  requires a[0] != SpecialValue && a[1] != SpecialValue;
{
  var i: int, v: MyValue;

  i := 0;
  while (i < N) {
    assert 0 <= i;
    assert i < N;
    v := a[i];
    assert a[i] != SpecialValue;
    i := i + 1;
  }
}

