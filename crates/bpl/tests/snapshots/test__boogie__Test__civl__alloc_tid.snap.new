  requires {:layer 1} tid == i;
{
  var t: int;

  call t := Read(tid, i);
  par Yield1() | Yield2(tid, t);
  call Write(tid, i, t + 1);
}

procedure {:atomic} {:layer 2, 2} AtomicAllocate() returns ({:linear "tid"} tid: int, i: int)
  modifies unallocated;
{
